if input.KeyCode == Enum.KeyCode[yoo] then
if uihide == false then
 uihide = true
 Main:TweenSize(UDim2.new(0, 0, 0, 0),"In","Quad",0,true)
else
 uihide = false
 Main:TweenSize(UDim2.new(0, 560, 0, 600),"Out","Quad",0,true)
end
end
 end)
	


 
